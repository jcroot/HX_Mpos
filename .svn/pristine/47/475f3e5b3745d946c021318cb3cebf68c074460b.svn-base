<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="120dp"
    android:layout_height="wrap_content"
    android:background="@drawable/popupwindow_bg_up_right_unselected"
    android:orientation="vertical"
    android:paddingBottom="5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp" >

    <TextView
        android:id="@+id/pop_tv_all"
        android:drawableRight="@drawable/ok32"
        style="@style/pop_tradelist_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="全部" />

    <View
        style="@style/common_line_horizontal_style"
        android:layout_width="wrap_content"
        android:layout_height="0.5dp" />

    <TextView
        android:id="@+id/pop_tv_cashin"
        style="@style/pop_tradelist_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="收款" />

    <View
        style="@style/common_line_horizontal_style"
        android:layout_width="wrap_content"
        android:layout_height="0.5dp" />

    <TextView
        android:id="@+id/pop_tv_consume"
        style="@style/pop_tradelist_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="消费" />

    <View
        style="@style/common_line_horizontal_style"
        android:layout_width="wrap_content"
        android:layout_height="0.1dp" />

    <TextView
        android:id="@+id/pop_tv_withdraw"
        style="@style/pop_tradelist_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="提现" />

    <View
        style="@style/common_line_horizontal_style"
        android:layout_width="wrap_content"
        android:layout_height="0.1dp" />

    <TextView
        android:id="@+id/pop_tv_time"
        style="@style/pop_tradelist_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="时间" />

</LinearLayout>