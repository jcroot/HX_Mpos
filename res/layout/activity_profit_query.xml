<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/com.td.app.pay.hx"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.td.app.pay.k7.ProfitQueryActivity" >

    <include layout="@layout/layout_title"/>

    <TextView
        android:id="@+id/tv_date_during"
        android:visibility="gone"
        android:textColor="#616161"
        android:background="#f5f5f5"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="2015-59" />

    <com.lk.td.pay.wedget.CustomListView
        android:id="@+id/listview_profit_query"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </com.lk.td.pay.wedget.CustomListView>

</LinearLayout>